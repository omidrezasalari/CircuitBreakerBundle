{
  "name": "omidrezasalari/circuit-breaker-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle implementing the Circuit Breaker pattern.",
  "autoload": {
    "psr-4": {
      "Omidrezasalari\\CircuitBreakerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omidrezasalari\\CircuitBreakerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "predis/predis": "*",
    "ext-apcu": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  }
}
